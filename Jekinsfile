pipeline{
  agent any
  
  stages{
    stage('checkout'){
      steps {
        echo 'Hii'
      }
    }
    stage('build'){
      steps {
        echo 'Build stage'
      }
    }
    stage('deploy'){
      steps {
        echo 'Deploy stage'
      }
    }
    stage('test'){
      steps {
        echo 'Test stage'
      }
    }
}
